{
    "swagger": "2.0",
    "info": {
        "description": "Реализация домашних заданий для курса",
        "title": "ML integration course",
        "contact": {},
        "version": "0.0.3"
    },
    "host": "localhost:7577",
    "basePath": "/",
    "paths": {
        "/calculate": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Вычисление суммы",
                "parameters": [
                    {
                        "description": "Объект с числами",
                        "name": "numbers",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "integer"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "integer"
                            }
                        }
                    }
                }
            }
        },
        "/query": {
            "post": {
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Выполнить SQL по CSV",
                "parameters": [
                    {
                        "type": "file",
                        "description": "CSV-файл",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "SQL-запрос",
                        "name": "sql",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        }
    }
}